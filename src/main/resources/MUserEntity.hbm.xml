<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.panyunyi.HealthyLifeMain.entity.MUserEntity" table="m_user" schema="healthylifemain">
        <id name="userId">
            <column name="userId" sql-type="int(11)"/>
        </id>
        <property name="userName">
            <column name="userName" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="userPassword">
            <column name="userPassword" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="picSrc">
            <column name="picSrc" sql-type="varchar(20)" length="20" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>