<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.panyunyi.HealthyLifeMain.entity.MStepEntity" table="m_step" schema="healthylifemain">
        <id name="userId">
            <column name="userId" sql-type="int(11)"/>
        </id>
        <property name="steps">
            <column name="steps" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="currentDate">
            <column name="currentDate" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>